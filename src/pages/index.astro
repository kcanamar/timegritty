---
import Base from "../layouts/Base.astro"

// NEON .astro config
import { neon } from "@neondatabase/serverless"

const sql = neon(import.meta.env.DATABASE_URL);
const response = await sql`SELECT version()`;
const data = response[0].version;

---

<Base>
	<h1>TimeGritty</h1>
	<section> 
		{data}
	</section>
</Base>

